type Service {
    id: ID! @id
    createdAt: DateTime! @createdAt
    updatedAt: DateTime! @updatedAt
    name: String!
    cost: String!
    postedBy: User
    appointments: [Appointment!]!
}

type User{
    id: ID! @id
    name: String!
    email: String! @unique
    password: String!
    services: [Service!]!
    appointments: [Appointment!]!
}

type Appointment{
    id: ID! @id
    service: Service!
    user: User!
}